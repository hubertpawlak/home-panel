{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "npm:dev",
      "command": "npm run dev",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "name": "npm:debug",
      "request": "launch",
      "runtimeArgs": [
        "--node-options",
        "--inspect=13333",
        "run",
        "dev"
      ],
      "attachSimplePort": 13333,
      "runtimeExecutable": "npm",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "node"
    },
  ]
}