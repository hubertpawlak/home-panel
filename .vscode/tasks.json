{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "eslint",
			"presentation": {
				"reveal": "silent",
				"clear": true
			},
			"problemMatcher": [
				"$eslint-stylish"
			],
			"label": "eslint"
		},
		{
			"type": "shell",
			"label": "supabase:generate types",
			"command": [
				"npx -y supabase@latest gen types typescript --local --schema public > types/supabase.ts"
			],
			"problemMatcher": []
		},
		{
			"type": "shell",
			"label": "supabase:start",
			"command": [
				"npx -y supabase@latest start"
			],
			"problemMatcher": []
		},
		{
			"type": "shell",
			"label": "supabase:stop",
			"command": [
				"npx -y supabase@latest stop"
			],
			"problemMatcher": []
		}
	]
}